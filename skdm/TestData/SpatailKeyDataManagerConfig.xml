<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<!--
		Default URL information is used for any action that doesn't override the value
		Given with either organizationName or clusterDomainUrl
		* organizationName - Will do a cluster lookup to find the clusterDomainUrl before authentication
		* clusterDomainUrl - Used directly for authentication and actions
	-->
	<!--    
	<organizationName>dev3</organizationName>
	-->
	<clusterDomainUrl>http://dev.spatialkey.com/</clusterDomainUrl>

	<!--
		Default authentiction information is used for any action that doesn't override the value
		Given with either userName and password or apiKey and userId
		* apiKey and userId - The keys to use to authenticate
		* userName and password - The user name and password to use, not as secure as using IDs
	-->
	<apiKey>xxx</apiKey>
	<userId>xxx</userId>
	<!--
	<userName>user@spatialkey.com</userName>
	<password>xxx</password>
	-->

	<!-- A list of actions to perform, each action can override organizationName, clusterDomainUrl, apiKey, userId, username, or password -->	
	<actions>
		<action name="csv example">
		  <action>overwrite</action>
		  <dataPath>SalesData.csv</dataPath>
		  <xmlPath>SalesData.xml</xmlPath>
		  <runAsBackground>true</runAsBackground>
		  <notifyByEmail>true</notifyByEmail>
		  <addAllUsers>false</addAllUsers>
		</action>
		<action name="shape example">
		  <action>poly</action>
		  <dataPath>110thCongressionalDistrictShapefile.zip</dataPath>
		  <datasetName>110th Congressional District</datasetName>
		  <datasetId></datasetId>
		</action>
	</actions>
</config>

