<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<!-- See http://support.spatialkey.com/dmapi for more information -->
	<!-- 
		The organization name and cluster url.
	-->
	<organizationName>xxx</organizationName>
	<clusterDomainUrl>https://xxx.spatialkey.com/</clusterDomainUrl>
	
	<!--
		Default authentication information is used for any action that doesn't override the value
		Given with either userName and password or apiKey and userId
		* apiKey and userId - The keys to use to authenticate
		* userName and password - The user name and password to use, not as secure as using IDs
	-->
	<apiKey>xxx</apiKey>
	<userId>xxx</userId>
	<!--
	<userName>user@xxx.com</userName>
	<password>xxx</password>
	-->

	<!-- A list of actions to perform, each action  -->	
	<actions>
		<action name="csv example">
			<!-- Can override organizationName, clusterDomainUrl, apiKey, userId, username, or password -->
			<type>overwrite</type>
			<dataPath>SalesData.csv</dataPath>
			<xmlPath>SalesData.xml</xmlPath>
			<runAsBackground>true</runAsBackground>
			<notifyByEmail>true</notifyByEmail>
			<addAllUsers>false</addAllUsers>
		</action>
		<action name="shape example">
			<!-- Can override organizationName, clusterDomainUrl, apiKey, userId, username, or password -->
			<type>poly</type>
			<dataPath>110thCongressionalDistrictShapefile.zip</dataPath>
			<!-- send datasetName for new dataset, datasetId if updating existing dataset -->
			<datasetName>110th Congressional District</datasetName>
			<!--
			<datasetId>xxx</datasetId>
			-->
		</action>
	</actions>
</config>

